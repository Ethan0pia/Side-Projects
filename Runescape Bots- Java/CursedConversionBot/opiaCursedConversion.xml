<manifest>
    <main-class>com.ethan0pia.bots.CursedConversionBot.CursedConversionBot</main-class>
    <name>OpiaCursedConversion</name>
    <tag-line>Converts cursed energy</tag-line><!--Max of 50 chars-->
    <description>Converts all cursed energy in inventory to other energy</description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>RS3</game>
    </compatibility>
    <categories>
        <category>DIVINATION</category>
        <category>MONEYMAKING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>OpiaCursed</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Cursed</tag>
        <tag>div</tag>
        <tag>money</tag>
    </tags>
    <resources>
        <resource>com/ethan0pia/bots/cursedConversionBot/ui/CursedConversionUI.fxml</resource>
    </resources>
</manifest>