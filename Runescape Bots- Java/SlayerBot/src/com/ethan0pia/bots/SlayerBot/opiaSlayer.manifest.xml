<manifest>
  <main-class>com.ethan0pia.bots.SlayerBot.OpiaSlayer</main-class>
  <name>OpiaSlayer</name>
  <tag-line>Does Slayer Tasks</tag-line>
  <description>Completes slayer tasks and gets another task.</description>
  <version>0.1</version>
  <compatibility>
    <game-type>RS3</game-type>
  </compatibility>
  <categories>
      <category>SLAYER</category>
      <category>MONEYMAKING</category>
    <category>COMBAT</category>
  </categories>
  <!--Required to publish on the bot store-->
  <internal-id>Ethan0piaSlayer</internal-id>
  <!--The rest are optional-->
  <open-source>false</open-source>
  <hidden>false</hidden>
  <!--If you need to hide it from the bot store for maintenance-->
